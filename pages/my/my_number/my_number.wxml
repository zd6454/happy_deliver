<!--pages/my/my_number/my_number.wxml-->
<view class="page">
   <view class="body">
       <view class="card" >
          <view  wx:for="{{items}}" wx:key="index" wx:for-index='index' >
              <view class="setting">
                    <view class="head">{{item.title}}</view>
                    <view class="content">
                       <view class="number">序号：{{item.number}}</view>
                       <view class="time">{{item.time}}</view>
                    </view>
                    <view class="foot"> {{item.day}}</view>
              </view>
              <view class="line"></view>
          </view>
       </view>
   </view>
</view>
